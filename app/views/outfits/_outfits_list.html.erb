<div>
  <div class="card-section">
    <% @outfits.sort.each do |outfit| %>
      <div class="card-product">
        <% if outfit.photo.attached? %>
          <%= link_to outfit_path(outfit) do %>
            <div class="product-photo" style="background-image: url('<%= cl_image_path outfit.photo.key, height: 120, crop: :fill %>')">
            </div>
          <% end %>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
